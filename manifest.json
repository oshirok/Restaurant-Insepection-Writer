{
  "name": "jQuery DOM",
  "version": "1",
  "manifest_version": 2,
  "description": "Manipulate the DOM when the page is done loading",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.11.3.min.js", "jquery-ui.min.js", "background.js"],
    "css": ["jquery-ui.min.css", "custom.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}